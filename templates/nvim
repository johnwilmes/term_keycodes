-- Automatically generated by term_keycodes script

function set_default(object, key)
    if rawget(object, key) ~= nil then
        return rawget(object, key)
    end
    rawset(object, key, {})
    return rawget(object, key)
end

local mod_map = {}
setmetatable(mod_map, {__index=set_default})

local function get_modifier_bits(modifiers)
    local mod_values = {S=1, A=2, C=4, D=8}
    local mod_bits = 0
    for mod, _ in pairs(modifiers) do
        if mod_values[mod] then
            mod_bits = mod_bits + mod_values[mod]
        end
    end
    return mod_bits
end

local function get_codepoint(key, modifiers)
    modifiers = modifiers or {}
    local mod_bits = get_modifier_bits(modifiers)
    return mod_map[key][mod_bits]
end

### REMAP TERMCODES ###

return {get_codepoint=get_codepoint}
